{
  "$schema": "./node_modules/ng-swagger-gen/ng-swagger-gen-schema.json",
  "swagger": "src/app/apis/openapi/api-v2.json",
  "output": "src/app/apis/applirest/api-v2",
  "prefix": "ApiV2Rest",
  "ignoreUnusedModels": false,
  "minParamsForContainer": 5,
  "sortParams": "desc",
  "defaultTag": "Api",
  "removeStaleFiles": true,
  "modelIndex": true,
  "serviceIndex": true,
  "apiModule": true,
  "enumModule": true,
  "generateExamples": false,
  "camelCase": false
}
